{
  "name": "product-review-app",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "preinstall": "cd ./static-server && npm install",
    "start-proxy": "cd ./static-server && npm start",
    "start-app": "react-scripts start",
    "start": "concurrently \"npm run start-proxy\" \"npm run start-app\"",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "prettier": "prettier --write '**/src/**/*.js?(x)'",
    "flow": "flow",
    "precommit": "lint-staged"
  },
  "dependencies": {
    "axios": "0.18.0",
    "concurrently": "^3.6.0",
    "lodash": "4.17.21",
    "moment": "2.22.2",
    "react": "16.4.1",
    "react-dom": "16.4.1",
    "react-infinite-scroller": "1.2.0",
    "react-redux": "5.0.7",
    "react-scripts": "1.1.4",
    "redux": "4.0.0",
    "redux-devtools-extension": "2.13.5",
    "redux-logger": "3.0.6",
    "redux-saga": "0.16.0",
    "semantic-ui-css": "2.3.3",
    "semantic-ui-react": "0.82.0"
  },
  "devDependencies": {
    "enzyme": "3.3.0",
    "enzyme-adapter-react-16": "1.1.1",
    "flow-bin": "0.76.0",
    "husky": "0.14.3",
    "jest-enzyme": "6.0.2",
    "lint-staged": "7.2.0",
    "prettier": "1.13.7",
    "prettierrc": "0.0.0-5"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write ",
      "git add"
    ]
  }
}
